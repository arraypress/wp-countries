{
  "name": "arraypress/wp-countries",
  "version": "1.0.0",
  "description": "A lightweight WordPress library for working with country codes and names with translation support.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-countries",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\Countries\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-countries/issues",
    "source": "https://github.com/arraypress/wp-countries"
  },
  "keywords": [
    "wordpress",
    "wp",
    "countries",
    "country-codes",
    "iso-3166",
    "country-list",
    "country-names",
    "i18n",
    "translation",
    "arraypress"
  ]
}